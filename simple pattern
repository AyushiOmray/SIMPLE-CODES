
#include <stdio.h>

int main()
{
    int n,i,j;
    
    for(i=0;i<7;i++){
            if(i==0||i==6){
             for(j=0;j<8;j++){
                 if(j==0||j==7){
                     printf("1");
                 }
                 else{
                     printf("0");
                 }
             }   
                  printf("\n");
            }
            
            if(i==1||i==5){
             for(j=0;j<8;j++){
                 if(j==0||j==1||j==6||j==7){
                     printf("3");
                 }
                 else{
                     printf("0");
                 }
             }   
                  printf("\n");
            }
            
        
        if(i==2||i==4){
             for(j=0;j<8;j++){
                 if(j==0||j==1||j==2||j==5||j==6||j==7){
                     printf("5");
                 }
                 else{
                     printf("0");
                 }
             }   
                  printf("\n");
            }
            
            if(i==3){
             for(j=0;j<8;j++){
                     printf("7");
            }   
                  printf("\n");
            }
    }

    return 0;
}
