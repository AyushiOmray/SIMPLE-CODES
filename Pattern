#include <stdio.h>

int main()
{
int n,p,i,j;
scanf("%d %d",&n,&p);

for(i=0;i<p;i++){
    for(j=0;j<=i;j++){
        printf("%d",n);
    }
    printf("\n");
    n++;
}
n=n-2;
for(i=p-1;i>0;i--){
    for(j=0;j<=i;j++)
    {
        printf("%d",n);
        
    }
    printf("\n");
    n--;
}
    return 0;
}
